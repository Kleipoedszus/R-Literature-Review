@article{ ISI:000185301300003,
Author = {Leschied, AW and Chiodo, D and Whitehead, PC and Hurley, D and Marshall,
   L},
Title = {{The empirical basis of risk assessment in child welfare: The accuracy of
   risk assessment and clinical judgment}},
Journal = {{CHILD WELFARE}},
Year = {{2003}},
Volume = {{82}},
Number = {{5}},
Pages = {{527-540}},
Month = {{SEP-OCT}},
Abstract = {{The importance of risk assessment is juxtaposed with the lack of
   empirical support regarding the validity of risk inventories. This study
   compared risk ratings of one risk assessment tool to decisions made by
   case managers. The researchers sampled 450 children and compared
   predictive utility of risk assessment to child protection decisions.
   Risk assessment was consistent with clinical judgment in 74\% to 81\% of
   cases, more than previously reported in studies of risk assessment
   validity. Further analyses identified discriminate functions at the
   instrument's category and individual-item levels. The results have
   implications for the validity of the instrument and its utility in child
   welfare.}},

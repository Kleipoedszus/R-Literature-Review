@article{ ISI:000078386900001,
Author = {Nurius, PS and Kemp, SP and Gibson, JW},
Title = {{Practitioners' perspectives on sound reasoning: Adding a
   worker-in-context component}},
Journal = {{ADMINISTRATION IN SOCIAL WORK}},
Year = {{1999}},
Volume = {{23}},
Number = {{1}},
Pages = {{1+}},
Abstract = {{Despite the increasing emphasis being placed on sound reasoning iii
   social work practice, there has been little empirical investigation of
   how practitioners understand this as taking concrete form or how it is
   impeded or facilitated by contextual and environmental factors. This gap
   in knowledge limits supervisors' and administrators' ability to under
   stand and manage environmental variables as these affect direct sei vice
   workers' practice reasoning and decision-making. This article reports a
   study on direct service workers that identifies: (I) what practitioners
   believe constitutes sound reasoning; (2) environmental factors that
   practitioners see as promoting or interfering with sound reasoning; and
   (3) strategies that practitioners believe useful in correcting problems
   in practice reasoning. Implications of these findings for the
   enhancement of sound reasoning, in human service agencies are discussed.}},
